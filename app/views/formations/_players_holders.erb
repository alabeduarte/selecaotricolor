<div class="call-up">
	<table class="zebra-striped">
		<tr>
		    <th><%= t(:player) %></th>
			<th><%= t(:position) %></th>
		</tr>
	<% @players_positions.each do |position| %>
	  	<tr>
		    <td><%= position.player.name %></td>
			<td><%= position.acronym %></td>
		</tr>
	<% end %>
	</table>
</div>